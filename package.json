{
  "name": "hubspot-multi-threading-score-app",
  "version": "1.0.0",
  "description": "HubSpot App to calculate and surface multi-thread stakeholder coverage scores on deals",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/ tests/",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\""
